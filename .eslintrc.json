{
    "ignorePatterns": [
      "node_modules",
      "dist",
      "coverage"
    ],
    "env": {
    },
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "args": "after-used",
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }